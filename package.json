{
  "name": "by-imperio-dog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/run-gen-client-photos.mjs",
    "prebuild": "node scripts/run-gen-client-photos.mjs && node scripts/contentlayer-build-wrapper.mjs",
    "dev": "cross-env NEXT_DISABLE_VERSION_CHECK=1 NEXT_TELEMETRY_DISABLED=1 next dev -p 3000",
    "build": "next build --no-lint",
    "build:stats": "npm run build && node scripts/collect-build-stats.mjs",
    "lh:serve": "next start -p 3000",
    "lh:run": "lighthouse http://localhost:3000/ --config-path=lighthouserc.json --output=json --output-path=./reports/lh-home.json || exit 0",
    "lh:ci": "npm run build && npx lhci autorun || echo 'LHCI optional'",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "seed": "tsx scripts/seed-demo.ts",
    "embeddings:index": "tsx scripts/embeddings-index.ts",
    "seo:audit": "tsx scripts/seo-audit.ts",
    "build:search-index": "tsx scripts/build-search-index.ts",
    "fix:encoding:dry": "node scripts/fix-encoding.mjs --dry",
    "fix:encoding": "node scripts/fix-encoding.mjs --write",
    "a11y:contrast": "node scripts/a11y-contrast.mjs",
    "test": "vitest --run",
    "test:unit": "vitest --run",
    "test:e2e": "playwright test || echo 'playwright not configured'",
    "test:e2e:chromium": "playwright test --project=chromium",
    "check:all": "npm run typecheck && npm run lint && npm run test && npm run check:encoding",
    "check:encoding": "node scripts/check-encoding.mjs",
    "postinstall": "echo \"Ready.\"",
    "vercel-build": "npm install && node scripts/contentlayer-build-wrapper.mjs && next build --no-lint"
  },
  "dependencies": {
    "@contentlayer/core": "^0.3.4",
    "@contentlayer/source-files": "^0.3.4",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@sanity/client": "^6.15.10",
    "@supabase/supabase-js": "^2.45.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "classnames": "^2.5.1",
    "contentlayer": "^0.3.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "groq": "^3.45.0",
    "lucide-react": "^0.540.0",
    "next": "14.2.4",
    "next-mdx-remote": "^5.0.0",
    "openai": "^5.23.2",
    "postcss": "^8.4.41",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-tsparticles": "^2.12.2",
    "recharts": "^3.1.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.12.6",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.33.4",
    "shiki": "^1.22.0",
    "tailwindcss": "^3.4.10",
    "tsparticles-slim": "^2.12.0",
    "typescript": "^5.6.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.16.5",
    "@types/react": "19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "axe-core": "^4.10.3",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "happy-dom": "^18.0.1",
    "jsdom": "^27.0.0",
    "node-fetch": "^2.7.0",
    "playwright": "^1.55.0",
    "shikiji": "^0.10.2",
    "supabase": "^2.48.3",
    "tsx": "^4.19.1",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20 <21"
  }
}
